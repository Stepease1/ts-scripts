    "url": "https://github.com/xylabs/ts-scripts.git"
  },
  "scripts": {
    "build": "rimraf bin && yarn lint && yarn lint-pkg && yarn compile",
    "clean": "rimraf *.tsbuildinfo && rm -r -f ./bin",
    "compile": "tsc -p ./tsconfig.build.json",
    "cycle": "eslint --rule 'import/no-cycle: [1, { maxDepth: 20 }]' ./src",
    "dead": "yarn ts-prune",
    "deploy": "yarn cycle && yarn build && yarn version patch && yarn npm publish",
    "deps": "yarn depcheck",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "lint-pkg": "npmPkgJsonLint .",
    "reinstall": "rimraf ./yarn.lock && rimraf ./yarn-error.log && rimraf ./node_modules && yarn install"
  },
  "engines": {
    "node": ">=14.16 <17"
  },
  "sideEffects": false,
  "version": "1.0.48",
  "packageManager": "yarn@3.1.1"
